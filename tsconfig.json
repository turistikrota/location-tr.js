{
    "compilerOptions": {
      "target": "esnext", // veya istediğiniz bir hedef sürüm
      "moduleResolution": "node",
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "module": "esnext",
      "outDir": "./dist",
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "resolveJsonModule": true
    },
    "include": ["index.ts"],
    "exclude": ["node_modules", "dist"]
  }
  